<!DOCTYPE html>
<html>
  <head>
    <title>SecondApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  </head>
	<body class="no-sidebar is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<div id="header" style="background-image: url(<%= asset_path 'header.jpg'%>);">
					<!-- Nav -->
						<nav id="nav">
							<ul>
								<li><%= link_to "Home", root_path %></li>
								<li><%= link_to "Test", pages_test_path %></li>
								<li><%= link_to "Log in", users_login_path %></li>
								<li><%= link_to "About", pages_about_us_path %></li>
								<li><a href="#">No Sidebar</a></li>
							</ul>
						</nav>
				</div>
			<!-- Main -->
				<div class="wrapper style1">
					<div class="container">
						<article id="main" class="special">
							<p>
								<%= yield %>
							</p>
						</article>
					</div>
				</div>
			<!-- Footer -->
				<div id="footer">
						<div class="row">
							<div class="col-12">
								<!-- Copyright -->
									<div class="copyright">
										<ul class="menu">
											<li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
										</ul>
									</div>
							</div>
						</div>
					</div>
				</div>
		</div>

	</body>
</html>